{
  "name": "aws-cdk-example",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -b src",
    "cdk:synth": "cdk synth",
    "cdk:diff": "cdk diff",
    "cdk:deploy": "[[ $CI == 'true' ]] && export CDK_APPROVAL='never' || export CDK_APPROVAL='broadening'; cdk deploy --require-approval $CDK_APPROVAL $npm_package_name",
    "cdk:bootstrap": "cdk bootstrap aws://$(aws sts get-caller-identity | jq -r .Account)/$AWS_REGION"
  },
  "engines": {
    "node": "14.13.0"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.69.0",
    "@aws-cdk/aws-apigateway": "1.69.0",
    "@aws-cdk/aws-dynamodb": "1.69.0",
    "@aws-cdk/aws-lambda": "1.69.0",
    "@aws-cdk/aws-s3-assets": "1.69.0",
    "@aws-cdk/core": "1.69.0",
    "@babel/core": "^7.0.0",
    "@types/aws-lambda": "8.10.64",
    "@types/aws-sdk": "2.7.0",
    "@types/fs-extra": "9.0.2",
    "@types/node": "14.11.10",
    "@types/react-dev-utils": "9.0.4",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "4.4.1",
    "aws-cdk": "1.69.0",
    "aws-sdk": "2.773.0",
    "eslint": "7.11.0",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "fork-ts-checker-webpack-plugin": "5.2.0",
    "fs-extra": "9.0.1",
    "jest": "26.6.0",
    "prettier": "2.1.2",
    "react-dev-utils": "10.2.1",
    "ts-loader": "8.0.6",
    "ts-node": "9.0.0",
    "typescript": "4.0.3",
    "uuid": "8.3.1",
    "webpack": "5.1.3",
    "webpack-cli": "4.1.0"
  }
