{
  "exclude": [".vscode", ".yarn", "bin", "config", "src", "cdk.out", "lib"],
  "formats": {
    "openapi": {
      "rootDir": "reference"
    },
    "markdown": {
      "rootDir": "docs",
      "include": ["README.md", "docs/**"]
    },
    "image": {
      "rootDir": "assets/images",
      "include": ["**"]
    },
    "json_schema": {
      "rootDir": "models"
    }
  }
}
