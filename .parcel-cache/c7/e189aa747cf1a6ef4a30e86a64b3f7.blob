<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
  </head>
  <body>
    <svg width="960" height="600"><g></g></svg>

    <script crossorigin="" src="dfc15c96f471bbffec082adb03dc5da8"></script>
    <script crossorigin="" src="406f22c25e287ca2b3b38601465be5d5"></script>

    <script crossorigin="" src="3edb287869b8e445862329869c2303d0"></script>
    <script data-parcel-key="263fcf9c22da1aee12747b4c1b0fb72e">
      const graph = dagreD3.graphlib.Graph().setGraph({});

      const svg = d3.select('svg');
      const inner = svg.select('g');

      const zoom = d3.zoom().on('zoom', function () {
        inner.attr('transform', d3.event.transform);
      });

      svg.call(zoom);

      const render = new dagreD3.render();

      render(inner, graph);

      const initialScale = 0.75;

      svg.call(
        zoom.transform,
        d3.zoomIdentity
          .translate(
            (svg.attr('width') - g.graph().width * initialScale) / 2,
            20
          )
          .scale(initialScale)
      );

      svg.attr('height', g.graph().height * initialScale + 40);

      fetch('template.dot')
        .then((response) => response.text())
        .then((template) => {
          graph.setGraph(graphlibDot.read(template));
        });
    </script>
  </body>
</html>
